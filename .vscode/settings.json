{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/*.pyc": true
  },
  "python.terminal.activateEnvInCurrentTerminal": true,
  "djangointellisense.projectRoot": "${workspaceFolder}/backend/app",
  "djangointellisense.settingsModule": "app.settings",
  "python.autoComplete.extraPaths": ["${workspaceFolder}/backend/app"],
  "python.analysis.extraPaths": ["${workspaceFolder}/backend/app"],
  "djangointellisense.debugMessages": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports.ruff": "explicit",
      "source.fixAll.ruff": "explicit",
      "source.document": "always"
    },
    "editor.formatOnPaste": true
  },
  "pylint.cwd": "${workspaceFolder}/backend",
  "workbench.colorTheme": "One Dark Pro Mix",
  "workbench.iconTheme": "eq-material-theme-icons-light",
  "workbench.colorCustomizations": {
    "activityBar.background": "#556B2F",
    "titleBar.activeBackground": "#6E8B3D",
    "titleBar.activeForeground": "#FAFBF7"
  },
  "windowColors.ðŸŒˆ BaseColor": "DarkOliveGreen",
  "workbench.preferredHighContrastColorTheme": "Default High Contrast",
  "workbench.preferredDarkColorTheme": "Default Dark Modern"
}
